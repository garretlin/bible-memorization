{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    // path alias config
    "baseUrl": ".",
    "paths": {
      // This needs to be mirrored in babel.config.js
      // Views is a directory with sub directories
      "views/*": ["src/views/*"],
      "src/*": ["src/*"]
    }
  }
}
